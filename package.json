{
  "name": "610-scouting-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client-install": "npm install --prefix client",
    "server": "nodemon ./bin/www --ignore 'client/'",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.0",
    "neo4j-driver": "^1.7.2",
    "properties-reader": "0.0.16",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pug": "2.0.0-beta11",
    "pug-bootstrap": "0.0.15",
    "request": "^2.88.0",
    "request-promise": "^4.2.3",
    "winston": "^3.1.0"
  }
}
